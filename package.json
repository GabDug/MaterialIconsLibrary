{
    "name": "ng-mat-icons",
    "version": "0.0.3",
    "scripts": {
        "build": "ng build",
        "build:production": "ng build --configuration production",
        "e2e": "ng e2e",
        "generate-icon-list": "tsc src/icon-data-generation/main.ts && node src/icon-data-generation/main.js ",
        "lint": "ng lint",
        "ng": "ng",
        "postinstall": "yarn run generate-icon-list",
        "prebuild": "yarn run generate-icon-list",
        "start": "ng serve",
        "test": "ng test"
    },
    "private": true,
    "license": "MIT",
    "repository": "github:Dugnychon/MaterialIconsLibrary",
    "bugs": "https://github.com/Dugnychon/MaterialIconsLibrary/issues",
    "engines": {
        "node": ">=18.13.0",
        "yarn": ">=1.20.0"
    },
    "dependencies": {
        "@angular/animations": "^17.1.2",
        "@angular/cdk": "^17.1.2",
        "@angular/common": "^17.1.2",
        "@angular/compiler": "^17.1.2",
        "@angular/core": "^17.1.2",
        "@angular/forms": "^17.1.2",
        "@angular/material": "^17.1.2",
        "@angular/platform-browser": "^17.1.2",
        "@angular/platform-browser-dynamic": "^17.1.2",
        "@angular/router": "^17.1.2",
        "@angular/service-worker": "^17.1.2",
        "@commitlint/cli": "^12.1.4",
        "@commitlint/config-conventional": "^12.1.4",
        "prettier": "^2.3.0",
        "pretty-quick": "^3.1.0",
        "rxjs": "~6.6.3",
        "standard-version": "^9.3.0",
        "tslib": "^2.2.0",
        "zone.js": "~0.14.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^17.1.2",
        "@angular/cli": "^17.1.2",
        "@angular/compiler-cli": "^17.1.2",
        "@types/jasmine": "~3.7.4",
        "@types/jasminewd2": "~2.0.9",
        "@types/node": "^20.11.16",
        "codelyzer": "^6.0.2",
        "husky": "^4.3.8",
        "jasmine-core": "~3.7.1",
        "jasmine-spec-reporter": "~7.0.0",
        "karma": "~6.3.2",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~4.0.1",
        "karma-jasmine-html-reporter": "^1.6.0",
        "protractor": "~7.0.0",
        "ts-node": "~10.9.0",
        "tslint": "~6.1.0",
        "typescript": "~5.3.3"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
            "pre-commit": "pretty-quick --staged"
        }
    }
}
